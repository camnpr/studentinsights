    "jest-fetch-mock": "^1.4.2",
    "jquery": "^3.2.1",
    "js-cookie": "^2.1.4",
    "lodash": "3.10.1",
    "moment": "2.19.3",
    "object-assign": "^4.1.1",
    "percentile": "^1.2.0",
    "promise-polyfill": "^6.0.2",
    "query-string": "^5.1.0",
